<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net6;net48;netcoreapp3.1;net5.0;netstandard2.0;net461</TargetFrameworks>
    <OutputType>Library</OutputType>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System.Web" Version="4.0.0" Condition="$(TargetFramework) == 'net48' OR $(TargetFramework) == 'net461'" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
    <PackageReference Include="Microsoft.Net.Http.Headers" Version="2.2.8" />
    <PackageReference Include="Verify" Version="18.0.0-beta.25" />
    <PackageReference Include="Argon" Version="0.1.0" />
    <PackageReference Include="Argon.Xml" Version="0.1.0" />
    <PackageReference Include="Microsoft.Extensions.DiagnosticAdapter" Version="3.1.30" />
    <PackageReference Include="System.Diagnostics.DiagnosticSource" Version="6.0.0" />
    <PackageReference Include="Nullable" Version="1.3.1" Condition="$(TargetFramework) == 'net48' OR $(TargetFramework) == 'net461' OR $(TargetFramework) == 'netstandard2.0'" PrivateAssets="all" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" Condition="$(Configuration) == 'Release'" />
    <PackageReference Include="ProjectDefaults" Version="1.0.80" PrivateAssets="all" />
  </ItemGroup>
</Project>